# - Config file for the AWLQtDemo package
# It defines the following variables
#  AWLQtDemo_INCLUDE_DIRS - include directories for AWLQtDemo
#  AWLQtDemo_LIBRARIES    - libraries to link against
#  AWLQtDemo_EXECUTABLE   - the test executable

# Compute paths
get_filename_component(AWLQtDemo_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

include (CMakeFindDependencyMacro)
find_package(Threads REQUIRED)

set(AWLQtDemo_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# This library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET AWLQtDemo AND NOT AWLQtDemo_BINARY_DIR)
	#include_directories("${AWLQtDemo_CMAKE_DIR}/sensor-gateway")
    include("${AWLQtDemo_CMAKE_DIR}/AWLQtDemoTargets.cmake")
endif()

# These are IMPORTED targets created by AWLQtDemoTargets.cmake
set(AWLQtDemo_EXECUTABLE AWLQtDemo)
