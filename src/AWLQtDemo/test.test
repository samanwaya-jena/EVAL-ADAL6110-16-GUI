QPushButton:hover, QComboBox[editable=&quot;false&quot;]:hover,
QComboBox[editable=&quot;true&quot;]::drop-down:hover, QMenuBar::item:hover, QSpinBox:hover, QAbstractSpinBox:hover {
    border-image: url(./Images/Widgets/pushbutton_hover.png) 5;
    border-width: 5;
}

QPushButton:pressed, QComboBox[editable=&quot;false&quot;]:on,
QComboBox[editable=&quot;true&quot;]::drop-down:on, QMenuBar::item:on , QSpinBox:pressed, QAbstractSpinBox:pressed{
    border-image: url(./Images/Widgets/pushbutton_pressed.png) 5;
    border-width: 5;
}

